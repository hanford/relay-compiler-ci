{
  "name": "relay-compiler-ci",
  "version": "1.0.0",
  "scripts": {
    "relay": "relay-compiler --src . --schema ./apps/backend/schema/main.graphql --artifactDirectory types"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "relay-compiler": "^13.0.1"
  },
  "relay": {
    "exclude": [
      "**/node_modules/**/*",
      "**/apps/backend/**/*"
    ],
    "language": "typescript",
    "noFutureProofEnums": true,
    "customScalars": {
      "NonEmptyString": "string",
      "EntityId": "EntityId",
      "UnixTimestamp": "Int",
      "DateTime": "DateString",
      "Date": "DateString",
      "Time": "string"
    }
  }
}
